doctype html
html
	head
		meta(name='viewport' content='width=600')
		title= title
		link(rel='stylesheet', href='/stylesheets/style.css/style.css')
		link(href='https://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css')
	body
		.content
			.top-border
				ul.nav-list
					li
						a#about(href='/posts/About') about
					li
						a#blog(href='/posts/') blog
					li
						a#contact(href='/posts/Contact') contact
					li
						a#resume(href="/download") resume
				.post-content.title-group
					h1.title Evan Kozliner
					img.portrait(src="/images/compressed-me.jpg")
			block content
	.footer
